<ns-header>
  <ul>
    <li>
      <h2><a href="/">Examples</a></h2>
      <ul>
        <li>
          <h3>Components</h3>
          <ul>
            {%- for component in collections.components %}
            <li><a href="{{ component.url }}">{{ component.fileSlug }}</a></li>
            {%- endfor -%}
          </ul>
        </li>
        <li>
          <h3>Pages</h3>
          <ul>
            {%- for page in collections.pages %}
            <li><a href="{{ page.url }}">{{ page.fileSlug }}</a></li>
            {%- endfor -%}
          </ul>
        </li>
        <li>
          <h3>Combinations</h3>
          <ul>
            {%- for combination in collections.combinations %}
            <li><a href="{{ combination.url }}">{{ combination.fileSlug }}</a></li>
            {%- endfor -%}
          </ul>
        </li>
      </ul>
    </li>
  </ul>
</ns-header>
